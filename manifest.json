{
    "manifest_version": 3,
    "name": "RouterInfo",
    "version": "1.5.5",
    "description": "Popup window for ZLT Home Broadbands.",
    "permissions": [
        "tabs",
        "storage"
    ],
    "action": {
        "default_icon": { "16": "images/icon16.png" },
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "content_scripts": [{
        "matches": ["http://192.168.8.1/*"],
        "js": ["scripts/content.js"]
    }],
    "options_ui": {
        "page": "options.html",
        "open_in_tab": true
    }
}